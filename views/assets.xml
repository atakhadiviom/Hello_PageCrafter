<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--
    Hello PageCrafter Theme - Asset Declarations
    Registers theme stylesheets and scripts
    -->

    <!-- Theme SCSS Assets -->
    <template id="hello_pagecrafter_assets_frontend" inherit_id="web.assets_frontend" name="Hello PageCrafter Assets">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/hello_pagecrafter/static/src/scss/main.scss"/>
        </xpath>
    </template>

</odoo>
